{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Requested delivery info",
  "description": "A prioritized list of suggested delivery methods, each with its own delivery address",
  "properties": {
    "SortOrder": {
      "description": "Value from 1 to 99 according to the order",
      "type": "string"
    },
    "Address": {
      "description": "Description of the delivery address.",
      "type": "array",
      "javaType": "org.folio.rest.jaxrs.model.DeliveryAddress",
      "items": {
        "type": "object"
      }
    }
  },
  "additionalProperties": false
}
